      <instance>
        <jor_review id="jor_review">
          <start/>
          <end/>
          <today/>
          <deviceid/>
          <subscriberid/>
          <simid/>
          <phonenumber/>
          <review/>
          <review.>
            <name/>
            <alert_id/>
            <signal/>
          </review.>
          <ebs/>
          <ebs_risk/>
          <ebs_risk_level/>
          <ebs_ihr/>
          <ebs_ihr_met/>
          <acknowledgement.>
            <contact/>
            <review/>
          </acknowledgement.>
          <lab.>
            <status/>
            <results/>
            <outcome/>
          </lab.>
          <case.>
            <approval/>
            <notes/>
          </case.>
          <meta>
            <instanceID/>
          </meta>
        </jor_review>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/jor_review/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/jor_review/end" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/jor_review/today" type="date"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/jor_review/deviceid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="subscriberid" nodeset="/jor_review/subscriberid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="simserial" nodeset="/jor_review/simid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/jor_review/phonenumber" type="string"/>
      <bind nodeset="/jor_review/review" readonly="true()" type="string"/>
      <bind nodeset="/jor_review/review./name" required="true()" type="string"/>
      <bind nodeset="/jor_review/review./alert_id" required="true()" type="string"/>
      <bind nodeset="/jor_review/review./signal" required="true()" type="select1"/>
      <bind nodeset="/jor_review/ebs" relevant=" /jor_review/review./signal =&quot;yes&quot;" required="true()" type="select1"/>
      <bind nodeset="/jor_review/ebs_risk" relevant="selected( /jor_review/ebs , 'confirm')" required="true()" type="select1"/>
      <bind nodeset="/jor_review/ebs_risk_level" relevant="selected( /jor_review/ebs_risk , 'risk')" required="true()" type="select1"/>
      <bind nodeset="/jor_review/ebs_ihr" relevant="selected( /jor_review/ebs , 'confirm')" required="true()" type="select"/>
      <bind nodeset="/jor_review/ebs_ihr_met" relevant="selected( /jor_review/ebs , 'confirm')" required="true()" type="select1"/>
      <bind nodeset="/jor_review/acknowledgement./contact" required="true()" type="string"/>
      <bind nodeset="/jor_review/acknowledgement./review" required="true()" type="string"/>
      <bind nodeset="/jor_review/lab./status" relevant="not(selected( /jor_review/ebs , 'confirm'))" required="true()" type="select1"/>
      <bind nodeset="/jor_review/lab./results" relevant="not(selected( /jor_review/ebs , 'confirm'))" required="true()" type="string"/>
      <bind nodeset="/jor_review/lab./outcome" relevant="not(selected( /jor_review/ebs , 'confirm'))" required="true()" type="select1"/>
      <bind nodeset="/jor_review/case./approval" relevant="not(selected( /jor_review/ebs , 'confirm'))" required="true()" type="select1"/>
      <bind nodeset="/jor_review/case./notes" relevant="not(selected( /jor_review/ebs , 'confirm'))" required="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/jor_review/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
